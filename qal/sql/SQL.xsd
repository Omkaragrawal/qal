<?xml version="1.0" ?>
<xsd:schema elementFormDefault="qualified" targetNamespace="http://www.optimalbpm.se/XMLschema/DAL/SQL" xmlns="http://www.optimalbpm.se/XMLschema/DAL/SQL" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:simpleType name="datatypes">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(integer|string|string(\(.*\))|serial|timestamp)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="db_types">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="MySQL"/>
      <xsd:enumeration value="PostgreSQL"/>
      <xsd:enumeration value="Oracle"/>
      <xsd:enumeration value="DB2"/>
      <xsd:enumeration value="SQLserver"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="and_or">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="AND"/>
      <xsd:enumeration value="OR"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="index_types">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UNIQUE"/>
      <xsd:enumeration value="CLUSTERED"/>
      <xsd:enumeration value="NONCLUSTERED"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="constraint_types">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NOT NULL"/>
      <xsd:enumeration value="UNIQUE"/>
      <xsd:enumeration value="PRIMARY KEY"/>
      <xsd:enumeration value="FOREIGN KEY"/>
      <xsd:enumeration value="CHECK"/>
      <xsd:enumeration value="DEFAULT"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="set_operator">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UNION"/>
      <xsd:enumeration value="INTERSECT"/>
      <xsd:enumeration value="DIFFERENCE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="data_source">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Flatfile_Dataset"/>
      <xsd:enumeration value="XPath_Dataset"/>
      <xsd:enumeration value="Matrix_Dataset"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="csv_dialects">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="excel-tab"/>
      <xsd:enumeration value="unix"/>
      <xsd:enumeration value="excel"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="statement">
    <xsd:choice>
      <xsd:element name="VerbCreateTable" type="VerbCreateTable"/>
      <xsd:element name="VerbCreateIndex" type="VerbCreateIndex"/>
      <xsd:element name="VerbSelect" type="VerbSelect"/>
      <xsd:element name="VerbCustom" type="VerbCustom"/>
      <xsd:element name="VerbInsert" type="VerbInsert"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="condition_part">
    <xsd:choice>
      <xsd:element name="ParameterConditions" type="ParameterConditions"/>
      <xsd:element name="ParameterCondition" type="ParameterCondition"/>
      <xsd:element name="Parameter_Expression" type="Parameter_Expression"/>
      <xsd:element name="VerbSelect" type="VerbSelect"/>
      <xsd:element name="Parameter_Expression" type="Parameter_Expression"/>
      <xsd:element name="ParameterString" type="ParameterString"/>
      <xsd:element name="ParameterNumeric" type="ParameterNumeric"/>
      <xsd:element name="ParameterIdentifier" type="ParameterIdentifier"/>
      <xsd:element name="ParameterCast" type="ParameterCast"/>
      <xsd:element name="ParameterFunction" type="ParameterFunction"/>
      <xsd:element name="ParameterIn" type="ParameterIn"/>
      <xsd:element name="ParameterDataset" type="ParameterDataset"/>
      <xsd:element name="ParameterCase" type="ParameterCase"/>
      <xsd:element name="ParameterSet" type="ParameterSet"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="tabular_expression_item">
    <xsd:choice>
      <xsd:element name="VerbSelect" type="VerbSelect"/>
      <xsd:element name="ParameterDataset" type="ParameterDataset"/>
      <xsd:element name="ParameterSet" type="ParameterSet"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterString">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterString" type="ParameterString"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterConstraint">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterConstraint" type="ParameterConstraint"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterColumndefinition">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterColumndefinition" type="ParameterColumndefinition"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterSource">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterSource" type="ParameterSource"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterWhen">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterWhen" type="ParameterWhen"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterIdentifier">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterIdentifier" type="ParameterIdentifier"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_Statement">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="statement" type="statement"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterOrderByItem">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterOrderByItem" type="ParameterOrderByItem"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterCondition">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterCondition" type="ParameterCondition"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterField">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterField" type="ParameterField"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_ParameterAssignment">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterAssignment" type="ParameterAssignment"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_expression_item">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="VerbSelect" type="VerbSelect"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Parameter_Expression" type="Parameter_Expression"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterString" type="ParameterString"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterNumeric" type="ParameterNumeric"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterIdentifier" type="ParameterIdentifier"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterCast" type="ParameterCast"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterFunction" type="ParameterFunction"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterIn" type="ParameterIn"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterDataset" type="ParameterDataset"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterCase" type="ParameterCase"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterSet" type="ParameterSet"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_tabular_expression_item">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="VerbSelect" type="VerbSelect"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterDataset" type="ParameterDataset"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ParameterSet" type="ParameterSet"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Array_list">
    <xsd:choice>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="*" type="*"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="Parameter_Expression">
    <xsd:all>
      <xsd:element minOccurs="0" name="expressionitems" type="Array_expression_item"/>
      <xsd:element minOccurs="0" name="operator" type="xsd:string"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="VerbCustom">
    <xsd:all>
      <xsd:element minOccurs="0" name="sql_postgresql" type="xsd:string"/>
      <xsd:element minOccurs="0" name="sql_oracle" type="xsd:string"/>
      <xsd:element minOccurs="0" name="sql_mysql" type="xsd:string"/>
      <xsd:element minOccurs="0" name="sql_db2" type="xsd:string"/>
      <xsd:element minOccurs="0" name="sql_sqlserver" type="xsd:string"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:element name="statement" type="statement"/>
</xsd:schema>

