<?xml version='1.0' encoding='UTF-8'?>
<merge>
  <mappings>
    <field_mappings>
      <field_mapping>
        <is_key>True</is_key>
        <src_column>ID</src_column>
        <src_datatype>integer</src_datatype>
        <src_cast_to>integer</src_cast_to>
        <transformations>
	        <if_empty order="2">0</if_empty>
	        <cast order="3">
		    	<dest_type>string</dest_type>
		       	<format_string/>
		     </cast>    
	        <replace order="4">
	        	<old>,</old>
	        	<new>.</new>
	        	<max/>
	        </replace>	   
	        <cast order="5">
		    	<dest_type>integer</dest_type>
		       	<format_string/>
		     </cast>     

        </transformations>
        <result_cast_to/>
        <dest_column>ID</dest_column>
      </field_mapping>
      <field_mapping>
        <is_key>False</is_key>
        <src_column>Name</src_column>
        <src_datatype>string</src_datatype>
        <src_cast_to>string</src_cast_to>
        <transformations>
	        <trim order="1"></trim>
	        <replace order="3">
	        	<old> </old>
	        	<new/>
	        	<max/>
	        </replace>	        
	        <cast order="4">
	        	<dest_type>string</dest_type>
		       	<format_string/>
		    </cast>     
        </transformations>
        <result_cast_to/>
        <dest_column>Name</dest_column>
      </field_mapping>
      <field_mapping>
        <is_key>False</is_key>
        <src_column>Changed</src_column>
        <src_datatype>timestamp</src_datatype>
        <src_cast_to>timestamp</src_cast_to>
        <transformations>
	        <cast order="1">
	        	<dest_type>string</dest_type>
		       	<format_string/>
		    </cast>     
	        <trim order="3"></trim>
	        <cast order="1">
	        	<dest_type>timestamp</dest_type>
		       	<format_string>%Y-%m-%d %H:%M:%S</format_string>
		    </cast> 	             
        </transformations>
        <result_cast_to></result_cast_to>
        <dest_column>Changed</dest_column>
      </field_mapping>
    </field_mappings>
    <table_mappings>
	  	<source_table>table_src</source_table>
	  	<destination_table>table_dst</destination_table>
    </table_mappings>
  </mappings>
  <settings>
  	<insert>true</insert>
  	<update>true</update>
  	<delete>true</delete> 	
  </settings>  
  <resources>
    <resource caption="destination" type="RDBMS" uuid="dest_uuid">
   	  <autocommit>True</autocommit>
      <database>bpmtest</database>
      <db_type>MySQL</db_type>
      <instance></instance>
      <password>test</password>
      <port></port>
      <server>localhost</server>
	  <username>bpmuser</username>
    </resource>
    <resource caption="source" type="RDBMS" uuid="source_uuid">
      <autocommit>True</autocommit>
      <database>bpmtest</database>
      <db_type>PostgreSQL</db_type>
      <instance></instance>
      <password>test</password>
      <port></port>
      <server>localhost</server>
      <username>bpmuser</username>
    </resource>

  </resources>
</merge>
