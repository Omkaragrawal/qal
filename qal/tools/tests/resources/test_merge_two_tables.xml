<merge>
    <mappings>
        <field_mappings>
            <field_mapping>
                <is_key>True</is_key>
                <src_reference>ID</src_reference>
                <src_datatype>integer</src_datatype>
                <transformations>
                    <if_empty order="2">0</if_empty>
                    <cast order="3">
                        <dest_type>string</dest_type>
                        <format_string></format_string>
                    </cast>
                    <replace order="4">
                        <old>,</old>
                        <new>.</new>
                        <max/>
                    </replace>
                    <cast order="5">
                        <dest_type>integer</dest_type>
                        <format_string></format_string>
                    </cast>
                </transformations>
                <dest_reference>ID</dest_reference>
            </field_mapping>
            <field_mapping>
                <is_key>False</is_key>
                <src_reference>Name</src_reference>
                <src_datatype>string</src_datatype>
                <transformations>
                    <trim order="1"></trim>
                    <cast order="4">
                        <dest_type>string</dest_type>
                        <format_string></format_string>
                    </cast>
                </transformations>
                <dest_reference>Name</dest_reference>
            </field_mapping>
            <field_mapping>
                <is_key>False</is_key>
                <src_reference>Changed</src_reference>
                <src_datatype>timestamp</src_datatype>
                <transformations>
                    <cast order="1">
                        <dest_type>string</dest_type>
                        <format_string></format_string>
                    </cast>
                    <trim order="3"></trim>
                    <cast order="1">
                        <dest_type>timestamp</dest_type>
                        <format_string>%Y-%m-%d %H:%M:%S</format_string>
                    </cast>
                </transformations>
                <dest_reference>Changed</dest_reference>
            </field_mapping>
        </field_mappings>
    </mappings>
    <settings>
        <insert>True</insert>
        <update>True</update>
        <delete>True</delete>
    </settings>
    <resources>
        <resource caption="destination" type="RDBMS" uuid="dest_uuid">
            <db_autocommit>True</db_autocommit>
            <db_databasename>bpmtest</db_databasename>
            <db_password>test</db_password>
            <db_server>localhost</db_server>
            <db_table_name>table_dst</db_table_name>
            <db_type>MySQL</db_type>
            <db_username>bpmuser</db_username>
        </resource>
        <resource caption="source" type="RDBMS" uuid="source_uuid">
            <db_autocommit>True</db_autocommit>
            <db_databasename>bpmtest</db_databasename>
            <db_password>test</db_password>
            <db_server>localhost</db_server>
            <db_table_name>table_src</db_table_name>
            <db_type>PostgreSQL</db_type>
            <db_username>bpmuser</db_username>
        </resource>
    </resources>
</merge>